        <ReturnType Name="NSID" Type="Edm.Int64" Nullable="false"/>
          <Annotation Term="OData.Description" String="Namespace Identifier"/>
      </Action>

      <Action Name="GetLogPage" IsBound="true">
        <Annotation Term="OData.Description" String="This action request the Log Page from the NVMe controller."/>
